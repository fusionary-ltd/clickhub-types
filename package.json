{
  "name": "@clickinvest/clickhub-types",
  "version": "0.0.1",
  "description": "ClickHub Types",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npx tsc -p . --yes",
    "postinstall": "npm run build"
  },
  "author": "ClickInvest",
  "license": "MIT",
  "devDependencies": {
    "typescript": "^4.9.3"
  },
  "exports": {
    ".": {
      "import": "./dist/",
      "require": "./dist/"
    },
    "./*": {
      "import": "./dist/*.js",
      "require": "./dist/*.js"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  }
}